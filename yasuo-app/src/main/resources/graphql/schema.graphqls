type Query {
    refreshToken(refreshToken: String!): AuthenticationResponse
    findAll: [Event]
}

type Mutation {
    login(loginRequest: LoginRequest!): AuthenticationResponse
}

input LoginRequest {
    username: String
    password: String
}

type AuthenticationResponse {
    accessToken: String
    refreshToken: String
    username: String
}

type Event {
    id: ID
    name: String
    content: String
    createdDate: String
}