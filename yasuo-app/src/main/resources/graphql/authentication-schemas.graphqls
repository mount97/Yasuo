extend type Query {
    refreshToken(refreshToken: String!): AuthenticationResponse
}

extend type Mutation {
    login(loginRequest: LoginRequest!): AuthenticationResponse
    logout: ResponseDto
}

type AuthenticationResponse {
    accessToken: String
}

type ResponseDto {
    code: Int
    success: Boolean
    message: String
}

input LoginRequest {
    username: String
    password: String
}
